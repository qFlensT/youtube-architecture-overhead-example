generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id String @id @default(nanoid())

  name        String
  description String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
